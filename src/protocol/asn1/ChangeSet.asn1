ChangeSetModule DEFINITIONS AUTOMATIC TAGS ::= BEGIN
    IMPORTS Version, Signature, BinaryData, Hash, Status
        FROM BasicModule;

ChangeData ::= CHOICE {
    asn1Change         ANY,
    binaryChange       BinaryData
}


Change ::= SEQUENCE {
    version             Version,
    target              Hash,
    changeData          ChangeData
}

ChangeSet ::= SEQUENCE {
    version             Version,
    transactionHash     Hash,
    accountHash         Hash,
    signature           Signature,
    applyStatus         Status,
    changes    SEQUENCE OF Change
}

END
