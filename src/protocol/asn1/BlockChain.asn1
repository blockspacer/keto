BlockChainModule DEFINITIONS AUTOMATIC TAGS ::= BEGIN
    IMPORTS Number, Hash, Version, Signature
        FROM BasicModule
        Account 
        FROM AccountModule
        Action
        FROM ActionModule
        Transaction, SignedTransaction, ChangeSet
        FROM TransactionModule;


Block ::= SEQUENCE {
    version             Version,
    id                  Hash,
    date                UTCTime,
    parent              Hash,
    transactions        SEQUENCE OF SignedTransaction,
    changeSet           SEQUENCE OF ChangeSet,
    merkelRoot          Hash
}

SignedBlock ::= SEQUENCE {
    version         Version,
    id              Hash,
    block           Block,
    hash            Hash,
    signature       Signature
}

END
